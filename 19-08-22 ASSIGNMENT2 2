#include<stdio.h>
int main()
{
    int a[10],f,n,i,sum=0,j,x;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
         }
         for(i=0;i<n;i++)
         {
             f=1;
             for(j=1;j<=a[i];j++)
             {
                 f=f*j;
            }
            sum=sum+f;
         }
         printf("factorial=%d",sum);
         return 0;
}
